{
  "manifest_version": 2,

  "name": "RottenTomatoes.com Context Menu",
  "description": "This extension adds a context menu for easy RottenTomatoes.com search.",
  "version": "0.1",
  "background" : { "scripts": ["handler.js"] },
  "permissions" : [
    "contextMenus",
    "tabs",
    "http://*/*",
    "https://*/*"
   ],
  "icons" : {
    "16" : "RTicon16.png",
    "48" : "RTicon48.png",
    "128" : "RTicon128.png"
  }
}